{% extends "cmuaudit/_base.anonymous.jinja" %}

{% block content %}
<div class="section">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h1 class="text-center">Upload completed!</h1>
            </div>
        </div>
        <div class="row">
            <div class="col-md-offset-3 col-md-6">
                <table class="table">
                    <thead>
                        <tr>
                            <th>Course ID</th>
                            <th>Year</th>
                            <th>Season</th>
                            <th>Unit</th>
                            <th>Grade</th>
                        </tr>
                    </thead>
                    <tbody>
                        {% for row_data in audit_data %}
                            <tr>
                                <th>
                                    {{row_data.course_id}}
                                </th>
                                <td>
                                    {{row_data.year}}
                                </td>
                                <td>
                                    {{row_data.season}}
                                </td>
                                <td>
                                    {{row_data.unit}}
                                </td>
                                <td>
                                    {{row_data.grade}}
                                </td>
                            </tr>
                        {% endfor %}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
{% endblock content %}